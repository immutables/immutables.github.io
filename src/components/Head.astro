---
interface Props {
  title: string
}

const {title} = Astro.props
---

<head>
  <meta charset="UTF-8"/>
  <meta name="keywords" content="java,immutable,object,record,factory,generate,annotation,json,mongo">
  <meta name="description" content="Java Annotation Processor for generating immutable classes, builders and fluent interfaces">
  <meta name="viewport" content="width=device-width"/>
  <link rel="icon" type="image/svg+xml" href="/gfx/favicon.svg"/>
  <link rel="preconnect" href="https://unpkg.com" crossorigin>
  <link rel="preload" href="https://unpkg.com/@fontsource/iosevka@5.2.5/files/iosevka-latin-400-normal.woff2" as="font" type="font/woff2" crossorigin>
  <meta name="generator" content={Astro.generator}/>
  <title>{title}</title>
</head>
