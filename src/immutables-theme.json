{
  "name": "Immutables Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#2e2f37",
    "editor.foreground": "#d3d5da"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6a6d77",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new",
        "keyword.other.important"
      ],
      "settings": {
        "foreground": "#799eba",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#5cb7ed",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#35c4b8",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#d3d5da"
      }
    },
    {
      "scope": [
        "variable",
        "variable.parameter",
        "variable.other"
      ],
      "settings": {
        "foreground": "#d3d5da"
      }
    },
    {
      "scope": [
        "meta.annotation",
        "storage.type.annotation",
        "punctuation.definition.annotation"
      ],
      "settings": {
        "foreground": "#a8b7d6"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "meta.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#799eba"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#a8b7d6"
      }
    },
    {
      "scope": [
        "string.quoted.double.xml",
        "string.quoted.single.xml"
      ],
      "settings": {
        "foreground": "#5cb7ed"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace",
        "punctuation.section"
      ],
      "settings": {
        "foreground": "#d3d5da"
      }
    },
    {
      "scope": [
        "constant.language",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#799eba"
      }
    }
  ]
}
